<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox minHeight="720" minWidth="1280" fx:id="loginPane" styleClass="login-pane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dziennik.controllers.LoginController">
    <Text text="Log in" style="-fx-font-size: 30px;"/>
    <TextField fx:id="emailField" promptText="E-mail address" />
    <PasswordField fx:id="passwordField" promptText="Password" />
    <Button fx:id="logInButton" text="Log in"/>
</VBox>